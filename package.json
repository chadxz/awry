{
  "name": "awry",
  "version": "8.1.0",
  "description": "An ARI websocket and API client library",
  "main": "index.js",
  "scripts": {
    "prettify": "prettier --write \"**/*.js\" \"!coverage/**\" \"!docs/**\"",
    "lint": "eslint .",
    "test": "npm run lint && mocha",
    "cover": "nyc --reporter=html --reporter=text mocha && open coverage/index.html",
    "ci-test": "npm run lint && npm run ci-mocha && npm run ci-upload-coverage",
    "ci-mocha": "nyc mocha",
    "ci-upload-coverage": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "docs": "esdoc"
  },
  "keywords": [
    "asterisk",
    "ari"
  ],
  "license": "MIT",
  "engines": {
    "node": ">=10.24.0",
    "npm": ">=6.14.11"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/chadxz/awry.git"
  },
  "nyc": {
    "temp-directory": "coverage/.nyc_output"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "debug": "^4.3.1",
    "esm": "^3.2.25",
    "retry": "^0.12.0",
    "ws": "^7.4.4"
  },
  "devDependencies": {
    "@types/mocha": "^8.2.2",
    "@types/ws": "^7.2.4",
    "acorn": "^8.1.0",
    "codecov": "^3.0.0",
    "env-test": "^1.0.0",
    "esdoc": "^1.0.4",
    "esdoc-standard-plugin": "^1.0.0",
    "eslint": "^7.23.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-promise": "^4.3.1",
    "mocha": "^8.3.2",
    "nock": "^13.0.11",
    "nyc": "^15.1.0",
    "prettier": "^2.2.1"
  }
}
