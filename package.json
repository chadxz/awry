{
  "name": "awry",
  "version": "8.0.0",
  "description": "An ARI websocket and API client library",
  "main": "index.js",
  "scripts": {
    "prettify": "prettier --write \"**/*.js\" \"!coverage/**\" \"!docs/**\"",
    "lint": "eslint .",
    "test": "npm run lint && mocha",
    "cover": "nyc --reporter=html --reporter=text mocha && open coverage/index.html",
    "ci-test": "npm run lint && npm run ci-mocha && npm run ci-upload-coverage",
    "ci-mocha": "nyc mocha",
    "ci-upload-coverage": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "docs": "esdoc"
  },
  "keywords": [
    "asterisk",
    "ari"
  ],
  "license": "MIT",
  "engines": {
    "node": ">=10",
    "npm": ">=6"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/chadxz/awry.git"
  },
  "nyc": {
    "temp-directory": "coverage/.nyc_output"
  },
  "dependencies": {
    "axios": "^0.20.0",
    "debug": "^4.0.1",
    "esm": "^3.2.22",
    "retry": "^0.12.0",
    "ws": "^7.2.5"
  },
  "devDependencies": {
    "@types/mocha": "^7.0.2",
    "@types/ws": "^7.2.4",
    "acorn": "^7.2.0",
    "codecov": "^3.0.0",
    "env-test": "^1.0.0",
    "esdoc": "^1.0.4",
    "esdoc-standard-plugin": "^1.0.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "mocha": "^7.1.2",
    "nock": "^12.0.3",
    "nyc": "^15.0.1",
    "prettier": "^2.0.5"
  }
}
