{
  "name": "awry",
  "version": "7.1.0",
  "description": "An ARI websocket and API client library",
  "main": "index.js",
  "scripts": {
    "prettify": "prettier --write \"**/*.js\" \"!coverage/**\" \"!docs/**\"",
    "lint": "eslint .",
    "test": "npm run lint && mocha",
    "cover": "nyc --reporter=html --reporter=text _mocha && open coverage/index.html",
    "ci-test": "npm run lint && npm run ci-mocha && npm run ci-upload-coverage",
    "ci-mocha": "nyc mocha",
    "ci-upload-coverage": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "docs": "esdoc"
  },
  "keywords": [
    "asterisk",
    "ari"
  ],
  "license": "MIT",
  "engines": {
    "node": ">=6.4.0",
    "npm": ">=5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/chadxz/awry.git"
  },
  "nyc": {
    "temp-directory": "coverage/.nyc_output"
  },
  "dependencies": {
    "debug": "^4.0.1",
    "esm": "^3.0.9",
    "request": "^2.75.0",
    "request-promise-native": "^1.0.3",
    "retry": "^0.12.0",
    "ws": "^6.0.0"
  },
  "devDependencies": {
    "codecov": "^3.0.0",
    "env-test": "^1.0.0",
    "esdoc": "^1.0.4",
    "esdoc-standard-plugin": "^1.0.0",
    "eslint": "^5.0.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "intelli-espower-loader": "^1.0.1",
    "mocha": "^5.0.0",
    "nock": "^10.0.0",
    "nyc": "^13.0.1",
    "power-assert": "^1.4.1",
    "prettier": "^1.11.1"
  }
}
